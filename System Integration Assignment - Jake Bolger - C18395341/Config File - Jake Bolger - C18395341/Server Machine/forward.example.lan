;
; BIND data file for local loopback interface
;
$TTL	604800
@	IN	SOA	ns.example.lan. root.example.lan. (
			      2		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;
@	IN	NS	ns.example.lan.
ns	IN	A	192.168.1.50
www	IN	A	192.168.1.50
@	IN	AAAA	::1
